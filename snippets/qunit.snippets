snippet t
	test("${1}", function() {
		${2}
	});
# For people who keep typing `it` from a bdd habit. ^_^
snippet it
	test("${1}", function() {
		${2}
	});
snippet th
	throws(function() {
		${3}
	}, /${1:ErrorPattern}/, "${2:message}");
snippet e
	equal(${1}, ${2}, "${3}");
snippet ex
	expect(${1});
snippet ok
	ok(${1:true}, "${2}");
snippet de
	deepEqual(${1}, ${2}, "${3}");
snippet m
	module('${1}', {
		setup: function() {
			${2}
		},
		teardown: function() {
			${3}
		}
	});
